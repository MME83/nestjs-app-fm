{
  "name": "nest-app-fm",
  "version": "0.0.1",
  "description": "finance manager api that monitors banks balances, receive transaction and shows some statistics",
  "author": "@mme83",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env STAGE=dev nest start --watch",
    "start:debug": "cross-env STAGE=dev nest start --debug --watch",
    "start:prod": "cross-env STAGE=prod node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env STAGE=dev jest --runInBand",
    "test:watch": "cross-env STAGE=dev jest --runInBand --watch",
    "test:cov": "cross-env STAGE=dev jest --runInBand --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env STAGE=dev jest --config ./test/jest-e2e.json",
    "docker-compose:dev": "docker-compose up --build",
    "docker-compose:test-e2e": "docker-compose -f docker-compose.yml -f test-e2e.yml up --build --exit-code-from api",
    "docker-compose:test": "docker-compose -f docker-compose.yml -f test.yml up --build --exit-code-from api",
    "docker-compose:test-cov": "docker-compose -f docker-compose.yml -f test-cov.yml up --build --exit-code-from api",
    "docker-compose:test-watch": "docker-compose -f docker-compose.yml -f test-watch.yml up --build",
    "docker-compose:prod": "docker-compose -f docker-compose.yml -f production.yml up --build",
    "typeorm4": "npx typeorm-ts-node-commonjs -d ./db/data-source.ts",
    "typeorm3": "npm run build && npx typeorm -d dist/db/data-source.js",
    "typeorm2": "node --require ts-node/register ./node_modules/typeorm/cli.js -d typeorm.config.ts",
    "typeorm1": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm": "npx typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- -d ./db/data-source.ts migration:generate ./db/migrations/init",
    "migration:run": "npm run typeorm -- -d ./db/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- -d ./db/data-source.ts migration:revert"
  },
  "dependencies": {
    "@nestjs/axios": "^1.0.1",
    "@nestjs/class-transformer": "^0.4.0",
    "@nestjs/class-validator": "^0.13.4",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "cross-env": "^7.0.3",
    "joi": "^17.7.0",
    "pg": "^8.8.0",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.8",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
